{
  "name": "backend-challenge",
  "version": "1.0.0",
  "description": "A service in Node.js that exposes an API which can be consumed from any client.",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --config test/integration/jest.json",
    "devStart": "nodemon app.js",
    "format": "prettier --write --ignore-unknown ./",
    "format:check": "prettier --check -c ./ --ignore-unknown",
    "lint": "eslint --ext .js .",
    "sls-deploy": "dazn aws exec -p dazn-discovery-dev -- sls deploy",
    "invoke-hello":  "sls invoke local --function hello",
    "invoke-reservation": "sls invoke local --function reservation" 
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleksandramaria/backend-challenge.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aleksandramaria/backend-challenge/issues"
  },
  "homepage": "https://github.com/aleksandramaria/backend-challenge#readme",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-plugin-jest": "^27.1.7",
    "got": "^12.5.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "2.8.1",
    "uuid": "^9.0.0",
    "serverless": "^3.26.0"
  }
